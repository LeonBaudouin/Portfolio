{% import "macro/title.twig" as title %}
<a 
    class="project"
    href='{{ getUrl('project_show', {id: project.getId()}) }}' 
    data-section-image='{{ getImagePath("gallery/" ~ project.getGalleryImage()) }}'
>
    {{ title.regular('h2', 'direction-center project-title', project.getName(), project.getType()) }}
</a>