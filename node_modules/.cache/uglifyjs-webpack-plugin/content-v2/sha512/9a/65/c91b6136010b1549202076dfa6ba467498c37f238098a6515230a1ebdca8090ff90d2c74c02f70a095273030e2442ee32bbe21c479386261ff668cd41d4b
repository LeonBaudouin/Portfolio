\n        }\\r\\n        else {\\r\\n            return -angle;\\r\\n        }\\r\\n    }\\r\\n    MathFunc.getAngle = getAngle;\\r\\n    function easeInOutQuad(progression) {\\r\\n        return progression < .5 ? 2 * progression * progression : -1 + (4 - 2 * progression) * progression;\\r\\n    }\\r\\n    MathFunc.easeInOutQuad = easeInOutQuad;\\r\\n    ;\\r\\n})(MathFunc || (MathFunc = {}));\\r\\n\",\"import { Palette } from \\\"../CustomTypes\\\";\\r\\nimport { MathFunc } from \\\"../Utils\\\";\\r\\nexport var DrawnElement;\\r\\n(function (DrawnElement) {\\r\\n    class TiltedSquare {\\r\\n        constructor(squareSettings) {\\r\\n            this.position = Object.assign({}, squareSettings.position);\\r\\n            this.processedPosition = Object.assign({}, squareSettings.position);\\r\\n            this.style = Palette.SquareDark;\\r\\n            this.size = squareSettings.size;\\r\\n            this.strokeSize = squareSettings.strokeSize;\\r\\n        }\\r\\n        Draw(ctx) {\\r\\n            ctx.strokeStyle = this.style;\\r\\n            ctx.lineWidth = this.strokeSize;\\r\\n            ctx.strokeRect(-this.size / 2, -this.size / 2, this.size, this.size);\\r\\n        }\\r\\n    }\\r\\n    DrawnElement.TiltedSquare = TiltedSquare;\\r\\n    class FollowingTiltedSquare extends TiltedSquare {\\r\\n        constructor(squareSettings, defaultAngle, sensitivity) {\\r\\n            super(squareSettings);\\r\\n            this.offsetAngle = defaultAngle;\\r\\n            this.previousAngle = defaultAngle;\\r\\n            this.squareAngle = defaultAngle;\\r\\n            this.cursorAngle = defaultAngle;\\r\\n            this.sensitivity = sensitivity;\\r\\n        }\\r\\n        UpdateFromCursor(e) {\\r\\n            this.UpdateFocusPoint({ x: e.clientX, y: e.clientY });\\r\\n        }\\r\\n        UpdateFocusPoint(focusPoint) {\\r\\n            this.previousAngle = this.cursorAngle;\\r\\n            let delta = MathFunc.getAngle(this.position, focusPoint) - t